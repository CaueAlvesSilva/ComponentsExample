ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AppComponents" "${PODS_CONFIGURATION_BUILD_DIR}/UIView+Shimmer"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AppComponents/AppComponents.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/UIView+Shimmer/UIView_Shimmer.framework/Headers"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -isystem "${PODS_CONFIGURATION_BUILD_DIR}/AppComponents/AppComponents.framework/Headers" -isystem "${PODS_CONFIGURATION_BUILD_DIR}/UIView+Shimmer/UIView_Shimmer.framework/Headers" -iframework "${PODS_CONFIGURATION_BUILD_DIR}/AppComponents" -iframework "${PODS_CONFIGURATION_BUILD_DIR}/UIView+Shimmer"
OTHER_LDFLAGS = $(inherited) -framework "AppComponents" -framework "UIView_Shimmer"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
